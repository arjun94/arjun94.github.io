<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">
		<class name="FriendRequest" table="friendRequest">
			<id name="id">
				<generator class="native"></generator>
			</id>
			<property name="date"/>
			<property name="status"/>
			<many-to-one name="fromUser" class="entity.User" cascade="none" lazy="false"></many-to-one>
			<many-to-one name="toUser" class="entity.User" cascade="none" lazy="false"></many-to-one>
		</class>
</hibernate-mapping>
